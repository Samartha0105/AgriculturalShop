      <style>
         /* Style for the dropdown menu */
         .dropdown {
             position: relative;
             display: inline-block;
         }
         .dropdown-content {
             display: none;
             position: absolute;
             background-color: #f9f9f9;
             min-width: 160px;
             box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
             z-index: 1;
         }
         .dropdown:hover .dropdown-content {
             display: block;
         }
         /* Style for flip cards */
.flip-card {
    background-color: transparent;
    width: 300px;
    height: auto; /* Adjust the height to auto to accommodate varying content */
    perspective: 1000px;
    display: inline-block;
    margin: 20px;
}

.flip-card-inner {
    width: 100%;
    height: 100%;
    text-align: center;
    transition: transform 0.6s;
    transform-style: preserve-3d;
}

.flip-card:hover .flip-card-inner {
    transform: rotateY(180deg);
}

.flip-card-front, .flip-card-back {
    width: 100%;
    height: 100%;
    backface-visibility: hidden;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.flip-card-front {
    background-color: #f1f1f1;
    border-radius: 10px;
}

.flip-card-back {
    background-color: #7596bd;
    transform: rotateY(180deg);
    border-radius: 10px;
}

/* Ensure the image in both sides is centered and has the same dimensions */
.flip-card img {
    width: 100%;
    height: auto;
    max-width: 100%;
    max-height: 100%;
}

/* Center text vertically */
.flip-card-back h2,
.flip-card-back p {
    margin: 0;
}

